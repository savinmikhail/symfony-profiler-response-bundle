{
  "name": "savinmikhail/symfony-profiler-response-bundle",
  "description": "Symfony Web Profiler panel to display JSON/text response bodies with size limits and guards (dev-only)",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "symfony/http-kernel": "^5.4 || ^6.0 || ^7.0",
    "symfony/http-foundation": "^5.4 || ^6.0 || ^7.0",
    "symfony/dependency-injection": "^5.4 || ^6.0 || ^7.0",
    "symfony/config": "^5.4 || ^6.0 || ^7.0",
    "symfony/yaml": "^5.4 || ^6.0 || ^7.0"
  },
  "require-dev": {
    "symfony/web-profiler-bundle": "^5.4 || ^6.0 || ^7.0"
  },
  "autoload": {
    "psr-4": {
      "SavinMikhail\\ResponseProfilerBundle\\": "src/"
    }
  },
  "extra": {
    "symfony": {
      "bundle": {
        "SavinMikhail\\ResponseProfilerBundle\\ResponseProfilerBundle": ["all"]
      }
    }
  }
}

