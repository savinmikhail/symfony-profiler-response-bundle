{
  "name": "savinmikhail/symfony-profiler-response-bundle",
  "description": "Symfony Web Profiler panel to display JSON/text response bodies with size limits and guards (dev-only)",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "symfony/http-kernel": "^5.4 || ^6.0 || ^7.0",
    "symfony/http-foundation": "^5.4 || ^6.0 || ^7.0",
    "symfony/dependency-injection": "^5.4 || ^6.0 || ^7.0",
    "symfony/config": "^5.4 || ^6.0 || ^7.0",
    "symfony/yaml": "^5.4 || ^6.0 || ^7.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.64",
    "phpstan/phpstan": "^1.11",
    "phpunit/phpunit": "^10.5 || ^11.0",
    "phpyh/coding-standard": "^3.0",
    "rector/rector": "^1.0",
    "savinmikhail/dist-size-optimizer": "^0.2.6",
    "symfony/framework-bundle": "^5.4 || ^6.0 || ^7.0",
    "symfony/routing": "^5.4 || ^6.0 || ^7.0",
    "symfony/web-profiler-bundle": "^5.4 || ^6.0 || ^7.0"
  },
  "autoload": {
    "psr-4": {
      "SavinMikhail\\ResponseProfilerBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "symfony": {
      "bundle": {
        "SavinMikhail\\ResponseProfilerBundle\\ResponseProfilerBundle": ["all"]
      }
    }
  },
  "scripts": {
    "cs:fix": "php-cs-fixer fix --ansi --verbose",
    "cs:check": "php-cs-fixer fix --dry-run --diff --ansi --verbose",
    "stan": "phpstan analyse --ansi -c phpstan.neon.dist",
    "rector": "rector process --ansi",
    "test": "phpunit --colors=always",
    "dist:archive": "php tools/dist-size.php",
    "dist:size": "php tools/dist-size.php"
  },
  "config": {
    "sort-packages": true
  },
  "prefer-stable": true
}
